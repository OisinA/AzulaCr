func is_prime(int x): bool {
    int i, divisors = 1, 0;
    while(i <= x / 2) {
        if(x % i == 0) {
            divisors = divisors + 1;
        }
        i = i + 1;
    }
    return divisors <= 1;
}

func main(): void {
    int i = 0;
    while(i < 100) {
        print("%d: %d%c", i, is_prime(i), 10);
        i = i + 1;
    }
    print("%c%c", 10, 13);
}