func fizzbuzz(int n) {
    if n > 100 {
        return;
    }
    if n % 3 == 0 && n % 5 == 0 {
        printf("fizzbuzz\n");
        fizzbuzz(n + 1);
        return;
    }
    if n % 3 == 0 {
        printf("fizz\n");
        fizzbuzz(n + 1);
        return;
    }
    if n % 5 == 0 {
        printf("buzz\n");
        fizzbuzz(n + 1);
        return;
    }

    printf("%d\n", n);
    fizzbuzz(n + 1);
}

func main: int {
    fizzbuzz(0);

    return 0;
}