func is_prime(int x): bool {
    int i = 1;
    int divisors = 0;
    while(i <= x / 2) {
        if(x % i == 0) {
            divisors = divisors + 1;
        }
        i = i + 1;
    }
    return divisors <= 1;
}

func main(): void {
    int i = 0;
    while(i < 10000) {
        printf("%d: %d", i, is_prime(i));
        puts("");
        i = i + 1;
    }
    printf("%c%c", 10, 13);
}