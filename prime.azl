func is_prime(int x): bool {
    int i = 1;
    int divisors = 0;
    while(i <= x) {
        if(x % i == 0) {
            divisors = divisors + 1;
        }
        i = i + 1;
    }
    return divisors == 2;
}

func main(): void {
    int i = 0;
    while(i < 1000) {
        if(is_prime(i)) {
            printf("%d is prime!%c", i, 10);
        }
        i = i + 1;
    }
}